{
  "name": "starter-ts-node",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "engines": {
    "node": "18.15"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "node .",
    "test": "NODE_ENV=test nyc mocha --watch",
    "test:coverage": "NODE_ENV=test nyc mocha && open './coverage/lcov-report/index.html'",
    "docker:build": "docker build . -t $DOCKER_IMAGE_TAG --build-arg NODE_VERSION=$npm_package_engines_node --cache-from $DOCKER_IMAGE_TAG",
    "linter:fix": "eslint ./src/** --ext .ts --fix --cache",
    "precommit": "eslint $(git diff --staged --name-only --diff-filter=RAM ./src) --ext .ts --fix-dry-run --fix-type problem --max-warnings 0 --cache",
    "prepush": "NODE_ENV=test mocha",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/amqplib": "^0.8.2",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node-schedule": "^2.1.0",
    "@types/sinon": "^10.0.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "husky": "^8.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
