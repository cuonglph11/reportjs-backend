    "amqplib": "^0.10.1",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.0",
    "typescript-json-schema": "^0.54.0"
  }
}
